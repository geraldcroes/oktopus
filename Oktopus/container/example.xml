<?xml version="1.0" encoding="UTF-8"?>
<container>
<!--  A simple shared Component with id == className-->
<component id="Foo" shared="true">
</component>

<!--  A simple unshared Component with id !== className-->
<component id="Foo2" classname="Foo" shared="false">
</component>

<!--  A simple shared component with arguments in its constructor -->
<component id="Foo3" classname="Foo">
 <constructor-argument value="premiere valeur" />
</component>

</container>